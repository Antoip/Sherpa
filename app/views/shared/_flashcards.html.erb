
  <script language="JavaScript">
    function showInput() {
        document.getElementById('display').innerHTML =
                    document.getElementById("user_input").value;
    }
  </script>
<% flashcards.each do |flashcard| %>

<div class="wrapper-flashcard">
        <div class="card-flashcard">
            <input type="checkbox" id="card1" class="more" aria-hidden="true">
            <div class="content">
                <div class="front-flashcard" >
                    <div class="inner">
                      <img class="icon-question-flashcard" src="data:image/svg+xml;utf8;base64," />
                      <div  class="question-flashcard">
                        <p><%=  flashcard.question %></p>
                      </div>
                        <form>
                          <textarea class="form-control" id="user_input" autofocus="" rows="4" placeholder="Enter your answer here (alt + enter to flip card)"  id="user_input"  type="text" name="message">
                          </textarea>
                        </form>
                        <label for="card1" class="button-submit-flashcard" aria-hidden="true" onclick="showInput();">
                            Answer
                        </label>
                    </div>
                </div>


                <div class="back-flashcard">
                  <div class="place-icon-answer">
                    <img class="icon-question-flashcard-backside" src="data:image/svg+xml;utf8;base64," />
                  </div>
                       <div class="flashcard-subject">
                        <h5>History
                        </h5>
                       </div>
                        <div class="info-flashcard">
                          </div>
                            <div class="description-flashcard">
                            <h8 >Your Answer:</h8>
                             <p><span id='display'></span></p>
                        </div>
                        <div class="description-flashcard">
                          <h8>The prof Answer:
                          </h8>
                          <p> <%= flashcard.answer %></p>
                        </div>
                        <div class="bottom-flashcard-button">
                        <label for="card1" class="button return" aria-hidden="true">
                            <i class="fas fa-arrow-left"></i>
                        </label>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    <% end %>
    </div>


