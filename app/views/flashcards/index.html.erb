<div class="container-cards-student-mr mt-5">
  <div id="stacked-cards-block" class="stackedcards stackedcards--animatable init">
    <div class="stackedcards-container">
      <div class="card-item">
        <div class="d-flex flex-column align-items-center front-flashcard">
          <img class="icon-question-flashcard mt-5" src="data:image/svg+xml;utf8;base64," />
          <div  class="question-flashcard mt-5">
            <p>Question</p>
          </div>
          <form class='flashcard-form'>
            <textarea class="form-control" id="user_input" autofocus="" rows="4" placeholder="Enter your answer here (alt + enter to flip card)"  id="user_input"  type="text" name="message">
            </textarea>
          </form>
          <label for="card1" class="button-submit-flashcard mt-5" aria-hidden="true" onclick="showInput();">
              Check Answer
          </label>
        </div>
      </div>
      <div class="card-item"><img src="https://picsum.photos/201/300" alt="Tree 3" /></div>
      <div class="card-item">C</div>
      <div class="card-item">D</div>
      <div class="card-item">E</div>
      <div class="card-item">F</div>
      <div class="card-item">G</div>
    </div>
    <div class="stackedcards--animatable stackedcards-overlay top">TOP</div>
    <div class="stackedcards--animatable stackedcards-overlay right">RIGHT</div>
    <div class="stackedcards--animatable stackedcards-overlay left">LEFT</div>
  </div>
  <div class="global-actions mt-5">
    <div class="left-action">Left</div>
    <div class="top-action" style="display:none;">Top</div>
    <div class="right-action">Right</div>
  </div>
</div>
