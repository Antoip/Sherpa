<div class="mobile-center-container">
  <div class="feedback-course-details d-flex w-90 justify-content-between">
    <h5><%= @course.material.name %></h5>
    <p><%= @course.user %></p> <!-- Penser a changer par user.full_name une fois les seeds changees -->
  </div>

  <div class="container">
    <div class="row">
      <div class="col-xs-12 col-sm-4 col-sm-offset-4">
        <%= render "form", course: @course ,feedback: @feedback %>
      </div>
    </div>
  </div>
</div>



